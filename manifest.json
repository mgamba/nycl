{
  "manifest_version": 2,

  "name": "Easy Thumbs",
  "description": "View thumbnails on craigslist without opening listings.",
  "version": "1.0",

  "permissions": [
    "http://images.craigslist.org/*"
  ],
  "content_scripts": [ {
      "matches": ["http://newyork.craigslist.org/*"],
      "js": ["dropdowns.js"]
    }
  ]
}
